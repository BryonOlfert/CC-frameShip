modem = peripheral.wrap("bottom")
peripheral.call("bottom", "transmit", 5,5, "request")
modem.open(5)
p1, p2, p3, p4, p5 = os.pullEvent("modem_message")
local coors = p5
cfile = fs.open("data/destination","w")
cfile.write(coors)
cfile.close()
cfile = fs.open("data/task","w")
cfile.write("move")
cfile.close()
shell.run("startup")