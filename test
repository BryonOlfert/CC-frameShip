function writer(x)
    term.clear()
    term.setCursorPos(1,1)
print(x)
end
writer("Enter X")
x = read()
writer("Enter Y")
y = read()
writer("Enter Z")
z = read()
local coors = {x,y,z}
cfile = fs.open("data/destination.lua","w")
cfile.write(textutils.serialise(coors))
cfile.close()
cfile = fs.open("data/task.lua","w")
cfile.write("move.lua")
cfile.close()
shell.run("startup")